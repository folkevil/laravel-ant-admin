          </a-menu-item>\n            <a-menu-divider v-if=\"menu\" />\n            <a-menu-item key=\"logout\" @click=\"handleLogout\">\n              <a-icon type=\"logout\" />\n              退出登录\n            </a-menu-item>\n          </a-menu>\n        </template>\n  </a-dropdown>\n\n  <span v-else>\n    <a-spin size=\"small\" :style=\"{ marginLeft: 8, marginRight: 8 }\" />\n  </span>\n\n</template>\n\n<script>\nimport { Modal } from 'ant-design-vue'\nexport default {\n    name: 'AvatarDropdown',\n    props: {\n        currentUser: {\n            type: Object,\n            default: () => null\n        },\n        menu: {\n            type: Boolean,\n            default: true\n        }\n    },\n    methods: {\n        handleToCenter() {\n            this.$router.push({ path: '/account/center' })\n        },\n        handleToSettings() {\n            this.$router.push({ name: 'MySettings' })\n        },\n        handleLogout(e) {\n            Modal.confirm({\n                title: '退出提示',\n                content: '您确定要退出系统吗？',\n                onOk: () => {\n                    return this.$store.dispatch('Logout').then(() => {\n                        this.$router.push({ name: 'login' })\n                    })\n                },\n                onCancel() {}\n            })\n        }\n    }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.ant-pro-drop-down {\n    /deep/ .action {\n        margin-right: 8px;\n    }\n    /deep/ .ant-dropdown-menu-item {\n        min-width: 160px;\n    }\n}\n</style>\n"]}]}