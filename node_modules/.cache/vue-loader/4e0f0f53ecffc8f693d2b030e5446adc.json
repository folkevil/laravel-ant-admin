       <a-menu-divider v-if=\"menu\" />\n        <a-menu-item key=\"logout\" @click=\"handleLogout\">\n          <a-icon type=\"logout\" />退出登录\n        </a-menu-item>\n      </a-menu>\n    </template>\n  </a-dropdown>\n  <span v-else>\n    <a-spin size=\"small\" :style=\"{ marginLeft: 8, marginRight: 8 }\" />\n  </span>\n</template>\n\n<script>\nimport { Modal } from 'ant-design-vue'\nimport { mapActions } from 'vuex'\n\nexport default {\n  name: 'AvatarDropdown',\n  props: {\n    currentUser: {\n      type: Object,\n      default: () => null\n    },\n    menu: {\n      type: Boolean,\n      default: true\n    }\n  },\n  methods: {\n    ...mapActions(['Logout']),\n    handleToCenter () {\n      this.$router.push({ path: '/account/center' })\n    },\n    handleToSettings () {\n      this.$router.push({ path: '/account/settings' })\n    },\n    handleLogout (e) {\n      Modal.confirm({\n        title: '询问',\n        content: '你确定要退出么?',\n        onOk: () => {\n          this.Logout()\n          this.$router.push({ path: '/user/login' })\n        },\n        onCancel () {}\n      })\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.ant-pro-drop-down {\n  /deep/ .action {\n    margin-right: 8px;\n  }\n  /deep/ .ant-dropdown-menu-item {\n    min-width: 160px;\n  }\n}\n</style>\n"]}]}