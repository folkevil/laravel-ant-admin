               style=\"width: 200px\"\n                v-decorator=\"[\n                    'name',\n                    {\n                    rules: [{ required: true, message: '请输入唯一识别码!' }]\n                    }\n                ]\"\n            />\n        </a-form-item>\n\n      <a-form-item label=\"角色名称\" hasFeedback :span=\"8\">\n        <a-input\n          placeholder=\"请入角色名称\"\n          style=\"width: 200px\"\n          v-decorator=\"[\n            'title',\n            {\n              rules: [{ required: true, message: '请输入角色名称!' }]\n            }\n          ]\"\n        />\n      </a-form-item>\n\n      <a-form-item label=\"状态\" hasFeedback :span=\"8\">\n        <a-select\n          style=\"width: 200px\"\n          v-decorator=\"[\n            'status',\n            {\n              rules: [{ required: true, message: '请选择状态!' }]\n            }\n          ]\"\n          placeholder=\"请选择\"\n        >\n          <a-select-option :value=\"1\">\n            启用\n          </a-select-option>\n          <a-select-option :value=\"0\">\n            禁用\n          </a-select-option>\n        </a-select>\n      </a-form-item>\n\n      <a-divider dashed> 权限设置 </a-divider>\n\n        <a-collapse :v-model=\"true\">\n            <a-collapse-panel header=\"请选择权限\">\n                <a-table :columns=\"columns\" :dataSource=\"rules\" :pagination=\"false\" size=\"small\" :showHeader=\"false\">\n                    <span slot=\"permission\" slot-scope=\"row\">\n                        <a-checkbox-group @change=\"$emit('changeAction', row)\" v-model=\"row.selected\">\n                            <a-checkbox :value=\"action.id\" :disabled=\"action.disabled\" v-for=\"(action, i) in row.actions\" :key=\"i\">\n                                {{ action.title }}\n                            </a-checkbox>\n                        </a-checkbox-group>\n                    </span>\n\n                    <span slot=\"checkAll\" slot-scope=\"row\">\n                        <a-checkbox\n                        :indeterminate=\"row.indeterminate\"\n                        :checked=\"row.checkedAll\"\n                        @change=\"$emit('checkAllActionChange', $event, row)\"\n                        ></a-checkbox>\n                    </span>\n                </a-table>\n\n                <a-checkbox slot=\"extra\" @click=\"e => {e.stopPropagation()}\">全部</a-checkbox>\n\n            </a-collapse-panel>\n        </a-collapse>\n    </a-form>\n  </a-modal>\n</template>\n\n<script>\nconst columns = [\n    {\n        title: '菜单',\n        dataIndex: 'title'\n    },\n    {\n        title: '权限',\n        scopedSlots: {\n            customRender: 'permission'\n        }\n    },\n    {\n        title: '全选',\n        scopedSlots: {\n            customRender: 'checkAll'\n        }\n    }\n]\n\nexport default {\n    props: {\n        rules: {\n            type: Array,\n            default: () => {\n                return []\n            }\n        },\n        treeData: {\n            type: Array,\n            default: () => {\n                return []\n            }\n        },\n        visible: {\n            type: Boolean,\n            default: false\n        },\n        confirmLoading: {\n            type: Boolean,\n            default: false\n        }\n    },\n    data() {\n        return {\n            columns\n        }\n    },\n    methods: {\n\n    },\n    beforeCreate() {\n        this.form = this.$form.createForm(this, {\n            name: 'role_from'\n        })\n    }\n}\n</script>\n\n<style scoped>\n    .ant-collapse /deep/ .ant-collapse{\n        background-color: none;\n    }\n</style>\n"]}]}